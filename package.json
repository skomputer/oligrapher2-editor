{
  "name": "oligrapher2-editor",
  "version": "0.1.0",
  "description": "Editing UI for Oligrapher 2",
  "main": "build/oligrapher.editor.min.js",
  "scripts": {
    "dev-build": "npm run && webpack-dev-server --devtool eval --progress --history-api-fallback --colors --hot --content-base build --port 9090 --config webpack.dev.config",
    "prod-build": "NODE_ENV=production webpack -p --display-modules --config webpack.prod.config.js",
    "test": "jest"
  },
  "author": "Matthew Skomarovsky",
  "license": "gpl",
  "bugs": {
    "url": "https://github.com/skomputer/oligrapher2-editor/issues"
  },
  "homepage": "https://github.com/skomputer/oligrapher2-editor/",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "lodash": "^3.10.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hotkeys": "^0.7.0"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.7.1",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1",
    "webpack-dev-server": "^1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/lodash"
    ]
  }
}
